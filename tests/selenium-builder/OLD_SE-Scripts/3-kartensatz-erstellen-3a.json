{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost:3000/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "newCardSet"
      }
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "id",
        "value": "newSetModal"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "createCardSet"
      }
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "id",
        "value": "helpNewSetName"
      }
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "id",
        "value": "helpNewSetDescription"
      }
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "id",
        "value": "helpNewSetCategory"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "newCardSetCancel"
      }
    },
    {
      "type": "get",
      "url": "http://localhost:3000/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "usr-name"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "logout"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}