        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="Use Case 3" enabled="true">
          <stringProp name="WebDriverSampler.script">WDS.sampleResult.sampleStart()

WDS.browser.get(&apos;http://localhost:3000/&apos;)

var pkg = JavaImporter(org.openqa.selenium, org.openqa.selenium.support.ui)
var wait = new pkg.WebDriverWait(WDS.browser, 10, 1000)

//login start
var googleLoginButton = WDS.browser.findElement(pkg.By.id(&apos;google&apos;))
wait.until(pkg.ExpectedConditions.presenceOfElementLocated(pkg.By.id(&apos;google&apos;)))
googleLoginButton.click()
                                    
wait.until(pkg.ExpectedConditions.presenceOfElementLocated(pkg.By.id(&apos;Email&apos;)))
var emailInput = WDS.browser.findElement(pkg.By.id(&apos;Email&apos;)) 
emailInput.sendKeys([&apos;arsnovaflashcards&apos;])
var passwordInput = WDS.browser.findElement(pkg.By.id(&apos;Passwd&apos;))
passwordInput.sendKeys([&apos;cardsARS!&apos;])
var loginButton = WDS.browser.findElement(pkg.By.id(&apos;signIn&apos;))
loginButton.click()
                  
wait.until(pkg.ExpectedConditions.elementToBeClickable(pkg.By.id(&apos;submit_approve_access&apos;)))
var acceptButton = WDS.browser.findElement(pkg.By.id(&apos;submit_approve_access&apos;))
acceptButton.click()
//login end

var newSetTitle = WDS.browser.findElement(pkg.By.id(&apos;new-set-input&apos;))
newSetTitle.sendKeys([&apos;Use Case 3&apos;])

var setAddButton = WDS.browser.findElement(pkg.By.id(&quot;newCardSet&quot;))
setAddButton.click()

wait.until(pkg.ExpectedConditions.presenceOfElementLocated(pkg.By.id(&apos;newSetDescription&apos;)))
var newSetDescription = WDS.browser.findElement(pkg.By.id(&apos;newSetDescription&apos;))
newSetDescription.sendKeys([&apos;Beschreibung&apos;])

var newSetCategory = WDS.browser.findElement(pkg.By.id(&apos;newSetCategory&apos;))
newSetCategory.sendKeys([&apos;Test&apos;])

var createSetButton = WDS.browser.findElement(pkg.By.id(&apos;createCardSet&apos;))
createSetButton.click()

WDS.sampleResult.sampleEnd()

var result = WDS.browser.findElement(pkg.By.linkText(&apos;Use Case 3&apos;))
if(result.empty) {
    WDS.sampleResult.successful = false
    WDS.sampleResult.responseMessage = &apos;There were no result returned&apos;
} else {
    result.click()
    
    var editSetButton = WDS.browser.findElement(pkg.By.id(&apos;set-details-controls-btn-editCard&apos;))
    editSetButton.click()
    
    wait.until(pkg.ExpectedConditions.presenceOfElementLocated(pkg.By.id(&apos;cardSetDelete&apos;)))
    var deleteSetButton = WDS.browser.findElement(pkg.By.id(&apos;cardSetDelete&apos;))
    deleteSetButton.click()
    deleteSetButton.click()
}

var emailButton = WDS.browser.findElement(pkg.By.id(&apos;usr-name&apos;))
emailButton.click()

var logoutButton = WDS.browser.findElement(pkg.By.id(&quot;logout&quot;))
logoutButton.click()</stringProp>
