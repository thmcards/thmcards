WDS.sampleResult.sampleStart()
WDS.browser.get(&apos;http://localhost:3000/&apos;)
//WDS.browser.get(&apos;http://localhost:3000/login&apos;)
//WDS.browser.get(&apos;http://localhost:3000/auth/google&apos;)

var subject1 = WDS.browser.findElement(pkg.By.id(&apos;subject1&apos;))
subject1.click()

WDS.sampleResult.sampleEnd()

// Lernkartenfach ist leer
var result = WDS.browser.findElement(pkg.By.id(&apos;clicktoflip2&apos;)
if(result.empty) {
    WDS.sampleResult.successful = true
    WDS.sampleResult.responseMessage = &apos;No more flashcards&apos;
}

var emailButton = WDS.browser.findElement(pkg.By.id(&apos;usr-name&apos;))
emailButton.click()

var logoutButton = WDS.browser.findElement(pkg.By.id(&apos;logout&apos;))
